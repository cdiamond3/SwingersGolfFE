{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./app/**/*"
  ],
  "extends": "expo/tsconfig.base"
}
